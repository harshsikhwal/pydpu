                    "description": "WildcardMatch options"
                },
                "code": {
                    "type": "integer",
                    "description": "match any ICMP code"
                },
                "code_wildcard": {
                    "enum": [
                        "WILDCARD_MATCH_NONE",
                        0,
                        "WILDCARD_MATCH_ANY",
                        256
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Wildcard Match",
                    "description": "WildcardMatch options"
                }
            },
            "additionalProperties": true,
