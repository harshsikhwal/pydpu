{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/AddressRange",
    "definitions": {
        "AddressRange": {
            "properties": {
                "ipv4_range": {
                    "$ref": "#/definitions/opi_api.network.opinetcommon.v1alpha1.IPRange",
                    "additionalProperties": true,
                    "description": "IPv4 address range"
                },
                "ipv6_range": {
                    "$ref": "#/definitions/opi_api.network.opinetcommon.v1alpha1.IPRange",
                    "additionalProperties": true,
                    "description": "IPv6 address range"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Address Range",
            "description": "AddressRange represents an IPv4 or IPv6 address range"
        },
        "opi_api.network.opinetcommon.v1alpha1.IPAddress": {
            "required": [
                "af"
            ],
            "properties": {
                "af": {
                    "enum": [
                        "IP_AF_UNSPECIFIED",
                        0,
                        "IP_AF_INET",
                        1,
                        "IP_AF_INET6",
                        2
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Ip Af",
                    "description": "IP address families"
                },
                "v4_addr": {
                    "type": "integer",
                    "description": "IPv4 address (-- api-linter: core::0141::forbidden-types=disabled     aip.dev/not-precedent: must use fixed32 --)"
                },
                "v6_addr": {
                    "type": "string",
                    "description": "IPv6 address",
                    "format": "binary",
                    "binaryEncoding": "base64"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "IP Address",
            "description": "IP Address object"
        },
        "opi_api.network.opinetcommon.v1alpha1.IPRange": {
            "required": [
                "low",
                "high"
            ],
            "properties": {
                "low": {
                    "$ref": "#/definitions/opi_api.network.opinetcommon.v1alpha1.IPAddress",
                    "additionalProperties": true,
                    "description": "starting IP address"
                },
                "high": {
                    "$ref": "#/definitions/opi_api.network.opinetcommon.v1alpha1.IPAddress",
                    "additionalProperties": true,
                    "description": "ending IP address"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "IP Range",
            "description": "IP Range"
        }
    }
}