{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/RuleL4Match",
    "definitions": {
        "RuleL4Match": {
            "properties": {
                "ports": {
                    "$ref": "#/definitions/opi_api.network.opinetcommon.v1alpha1.PortMatch",
                    "additionalProperties": true,
                    "description": "source and/or destination ports/ranges"
                },
                "type_code": {
                    "$ref": "#/definitions/opi_api.network.opinetcommon.v1alpha1.ICMPMatch",
                    "additionalProperties": true,
                    "description": "ICMP type/code match criteria"
                },
                "port_list": {
                    "$ref": "#/definitions/opi_api.network.opinetcommon.v1alpha1.PortListMatch",
                    "additionalProperties": true,
                    "description": "list of source and/or destination ports/ranges"
                },
                "icmp_match_list": {
                    "$ref": "#/definitions/opi_api.network.opinetcommon.v1alpha1.ICMPMatchList",
                    "additionalProperties": true,
                    "description": "list ICMP type/code match criteria"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Rule L 4 Match",
            "description": "L4 rule match criteria"
        },
        "opi_api.network.opinetcommon.v1alpha1.ICMPMatch": {
            "properties": {
                "type": {
                    "type": "integer",
                    "description": "match any ICMP type"
                },
                "type_wildcard": {
                    "enum": [
                        "WILDCARD_MATCH_NONE",
                        0,
                        "WILDCARD_MATCH_ANY",
                        256
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Wildcard Match",
                    "description": "WildcardMatch options"
                },
                "code": {
                    "type": "integer",
                    "description": "match any ICMP code"
                },
                "code_wildcard": {
                    "enum": [
                        "WILDCARD_MATCH_NONE",
                        0,
                        "WILDCARD_MATCH_ANY",
                        256
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Wildcard Match",
                    "description": "WildcardMatch options"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "ICMP Match",
            "description": "ICMPv4/ICMPv6 rule match criteria"
        },
        "opi_api.network.opinetcommon.v1alpha1.ICMPMatchList": {
            "required": [
                "icmp_match_list"
            ],
            "properties": {
                "icmp_match_list": {
                    "items": {
                        "$ref": "#/definitions/opi_api.network.opinetcommon.v1alpha1.ICMPMatch"
                    },
                    "type": "array",
                    "description": "ICMP type/code list"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "ICMP Match List",
            "description": "ICMP type/code match condition list"
        },
        "opi_api.network.opinetcommon.v1alpha1.PortListMatch": {
            "required": [
                "src_port_range",
                "dst_port_range"
            ],
            "properties": {
                "src_port_range": {
                    "items": {
                        "$ref": "#/definitions/opi_api.network.opinetcommon.v1alpha1.PortRange"
                    },
                    "type": "array",
                    "description": "list of source ports or port ranges"
                },
                "dst_port_range": {
                    "items": {
                        "$ref": "#/definitions/opi_api.network.opinetcommon.v1alpha1.PortRange"
                    },
                    "type": "array",
                    "description": "list of destination ports or port ranges"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Port List Match",
            "description": "TCP/UDP source and destination port list"
        },
        "opi_api.network.opinetcommon.v1alpha1.PortMatch": {
            "required": [
                "src_port_range",
                "dst_port_range"
            ],
            "properties": {
                "src_port_range": {
                    "$ref": "#/definitions/opi_api.network.opinetcommon.v1alpha1.PortRange",
                    "additionalProperties": true,
                    "description": "source port range"
                },
                "dst_port_range": {
                    "$ref": "#/definitions/opi_api.network.opinetcommon.v1alpha1.PortRange",
                    "additionalProperties": true,
                    "description": "destination port range"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Port Match",
            "description": "TCP/UDP rule match criteria"
        },
        "opi_api.network.opinetcommon.v1alpha1.PortRange": {
            "required": [
                "port_low",
                "port_high"
            ],
            "properties": {
                "port_low": {
                    "type": "integer",
                    "description": "range:0-65535"
                },
                "port_high": {
                    "type": "integer",
                    "description": "range:0-65535"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Port Range",
            "description": "PortRange object has low and high end of the port ranges"
        }
    }
}